{
  "compilerOptions": {
    "lib": ["ESNext"],
    "target": "ESNext",
    "module": "ESNext",
    "moduleDetection": "force",
    "baseUrl": ".",
    "paths": {
      "@models/*": ["src/models/*"],
      "@constants/*": ["src/constants/*"],
      "@websocket/*": ["src/websocket/*"],
      "@types": ["src/types.ts"]
    },
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true,
    "verbatimModuleSyntax": true,
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true
  }
}